        display: flex;
        gap: 1rem;
        margin-top: 1rem;
      }

      .social-link {
        width: 40px;
        height: 40px;
        background: white;
        border-radius: 10px;
        display: flex;
        align-items: center;
        justify-content: center;
        transition: all 0.3s ease;
        text-decoration: none;
        color: white;
      }
      .social-link-img {
        width: 22px;
        height: 22px;
        cursor: pointer;
      }
      .social-link:hover {
        background: var(--accent-color);
        transform: translateY(-3px);
      }

      .image {
        height: 457px;
        width: 440px;
        object-fit: contain;
      }
      @media (max-width: 480px) {
        .image {
          width: 304px;
          height: 349px;
        }
      }
      /* Animations */
      @keyframes fadeInLeft {
        from {
          opacity: 0;
          transform: translateX(-30px);
        }
        to {
          opacity: 1;
          transform: translateX(0);
        }
      }

      @keyframes fadeInRight {
        from {
          opacity: 0;
          transform: translateX(30px);
        }
        to {
          opacity: 1;
          transform: translateX(0);
        }
      }

      @keyframes fadeInUp {
        from {
          opacity: 0;
          transform: translateY(30px);
        }
        to {
          opacity: 1;
          transform: translateY(0);
        }
      }

      @keyframes float {
        0%,
        100% {
          transform: translateY(0px);
        }
        50% {
          transform: translateY(-20px);
        }
      }

      @keyframes countUp {
        from {
          opacity: 0;
          transform: translateY(20px);
        }
        to {
          opacity: 1;
          transform: translateY(0);
        }
      }

      /* Chatbot Icon Styles */
      .chatbot-icon {
        position: fixed;
        bottom: 70px;
        right: 30px;
        width: 60px;
        height: 60px;
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        color: white;
        font-size: 1.5rem;
        cursor: pointer;
        /* background: var(--accent-color); */
        background-color: white;
        box-shadow: var(--shadow-lg);
        z-index: 1000;
        box-shadow: 0 4px 10px rgba(0, 0, 0, 0.8);
        animation: floatUpDown 2s ease-in-out infinite;
        opacity: 0;
        transform: translateY(20px);
        transition: all 0.3s ease;
      }

      .chatbot-icon.show {
        opacity: 1;
        transform: translateY(0);
      }

      .chatbot-icon.hidden {
        opacity: 0;
        transform: translateY(20px);
        pointer-events: none;
      }

      /* Chatbot iframe container */
      .chatbot-container {
        position: fixed;
        bottom: 140px;
        right: 30px;
        width: 400px;
        height: 600px;
        background: white;
        border-radius: 12px;
        box-shadow: 0 10px 40px rgba(0, 0, 0, 0.2);
        z-index: 1001;
        display: none;
        overflow: hidden;
      }

      .chatbot-container.show {
        display: block;
        animation: slideUp 0.3s ease-out;
      }

      .chatbot-iframe {
        width: 100%;
        height: 100%;
        border: none;
        border-radius: 12px;
      }

      @keyframes slideUp {
        from {
          opacity: 0;
          transform: translateY(20px);
        }
        to {
          opacity: 1;
          transform: translateY(0);
        }
      }

      .chatbot-img {
        width: 50px;
        height: 66px;
      }

      @keyframes floatUpDown {
        0%,
        100% {
          transform: translateY(0);
        }
        50% {
          transform: translateY(-10px);
        }
      }

      .chatbot-icon:hover {
        transform: scale(1.5);
      }

      @keyframes bounce {
        0%,
        20%,
        50%,
        80%,
        100% {
          transform: translateY(0);
        }
        40% {
          transform: translateY(-10px);
        }
        60% {
          transform: translateY(-5px);
        }
      }

