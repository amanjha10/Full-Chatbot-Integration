    <script>
      // Mobile menu toggle
      const hamburger = document.getElementById("hamburger");
      const navMenu = document.getElementById("nav-menu");

      hamburger.addEventListener("click", () => {
        navMenu.classList.toggle("active");
        hamburger.classList.toggle("active");
      });

      // Close menu when clicking on a link
      document.querySelectorAll(".nav-link").forEach((link) => {
        link.addEventListener("click", () => {
          navMenu.classList.remove("active");
          hamburger.classList.remove("active");
        });
      });

      // Navbar scroll effect - Only for desktop and tablet
      window.addEventListener("scroll", () => {
        const navbar = document.getElementById("navbar");
        // Only apply scroll effects if not on mobile
        if (window.innerWidth > 768) {
          if (window.scrollY > 50) {
            navbar.classList.add("scrolled");
          } else {
            navbar.classList.remove("scrolled");
          }
        }
      });

      // Smooth scrolling for navigation links
      document.querySelectorAll('a[href^="#"]').forEach((anchor) => {
        anchor.addEventListener("click", function (e) {
          e.preventDefault();
          const target = document.querySelector(this.getAttribute("href"));
          if (target) {
            // Adjust offset for mobile (no fixed navbar) vs desktop (fixed navbar)
            const offsetTop =
              window.innerWidth <= 768
                ? target.offsetTop
                : target.offsetTop - 80;
            window.scrollTo({
              top: offsetTop,
              behavior: "smooth",
            });
          }
        });
      });

      // Intersection Observer for animations
      const observerOptions = {
        threshold: 0.1,
        rootMargin: "0px 0px -50px 0px",
      };

      const observer = new IntersectionObserver((entries) => {
        entries.forEach((entry) => {
          if (entry.isIntersecting) {
            entry.target.style.animation = "fadeInUp 0.8s ease-out forwards";
            entry.target.style.opacity = "1";
          }
        });
      }, observerOptions);

      // Observe elements for animation
      document
        .querySelectorAll(
          ".service-card, .testimonial-card, .feature-item, .why-choose-card"
        )
        .forEach((el) => {
          el.style.opacity = "0";
          observer.observe(el);
        });

      // Enhanced Counter Animation Function
      const animateCounters = (entries) => {
        entries.forEach((entry) => {
          if (entry.isIntersecting) {
            const counters = entry.target.querySelectorAll(".metric-number");

            counters.forEach((counter, index) => {
              // Add staggered delay for each counter
              setTimeout(() => {
                const target = parseInt(counter.getAttribute("data-target"));
                const duration = 2000; // 2 seconds
                const steps = 60; // Number of steps for smooth animation
                const increment = target / steps;
                let current = 0;
                let step = 0;

                // Add animation class
                counter.style.animation = "countUp 0.5s ease-out forwards";

                const updateCounter = () => {
                  if (step < steps) {
                    current += increment;
                    step++;

                    // Use easing function for smooth animation
                    const progress = step / steps;
                    const easeOutCubic = 1 - Math.pow(1 - progress, 3);
                    const displayValue = Math.floor(target * easeOutCubic);

                    // Format the number based on its value
                    if (
                      counter.parentElement
                        .querySelector(".metric-label")
                        .textContent.includes("%")
                    ) {
                      counter.textContent = displayValue + "%";
                    } else if (target > 50) {
                      counter.textContent = displayValue + "+";
                    } else {
                      counter.textContent = displayValue + "+";
                    }

                    requestAnimationFrame(updateCounter);
                  } else {
                    // Final value
                    if (
                      counter.parentElement
                        .querySelector(".metric-label")
                        .textContent.includes("%")
                    ) {
                      counter.textContent = target + "%";
                    } else {
                      counter.textContent = target + "+";
                    }
                  }
                };

                updateCounter();
              }, index * 200); // Stagger each counter by 200ms
            });

            // Unobserve after animation
            excellenceObserver.unobserve(entry.target);
          }
        });
      };

      // Create observer for excellence section
      const excellenceObserver = new IntersectionObserver(animateCounters, {
        threshold: 0.3,
        rootMargin: "0px",
      });

      // Observe the excellence section
      const excellenceSection = document.querySelector(".excellence");
      if (excellenceSection) {
        excellenceObserver.observe(excellenceSection);
      }

      // Add dynamic hover effects
      document
        .querySelectorAll(".service-card, .why-choose-card")
        .forEach((card) => {
          card.addEventListener("mouseenter", function () {
            this.style.transform = "translateY(-8px) scale(1.02)";
          });

          card.addEventListener("mouseleave", function () {
            this.style.transform = "translateY(-8px) scale(1)";
          });
        });

      // Enhanced metric hover effects
      document
        .querySelectorAll(".metric, .excellence_card")
        .forEach((metric) => {
          metric.addEventListener("mouseenter", function () {
            this.style.transform = "translateY(-5px) scale(1.05)";
          });

          metric.addEventListener("mouseleave", function () {
            this.style.transform = "translateY(-5px) scale(1)";
          });
        });

      // Parallax effect for floating shapes
      window.addEventListener("scroll", () => {
        const scrolled = window.pageYOffset;
        const rate = scrolled * -0.3;

        document.querySelectorAll(".shape").forEach((shape, index) => {
          const speed = (index + 1) * 0.1;
          shape.style.transform = `translateY(${scrolled * speed}px)`;
        });
      });

      // Add loading animation
      window.addEventListener("load", () => {
        document.body.style.opacity = "1";
        document.body.style.transition = "opacity 0.5s ease-in-out";
      });

      // Set initial body opacity
      document.body.style.opacity = "0";

      // Chatbot Icon Functionality removed - handled by chatbot.js

      // Handle chatbot icon click - removed conflicting handler
      // The onclick="toggleChatbot()" in HTML handles the click

      // Enhanced button interactions
      document
        .querySelectorAll(".btn-primary, .btn-secondary")
        .forEach((btn) => {
          btn.addEventListener("click", function (e) {
            // Create ripple effect
            const rect = this.getBoundingClientRect();
            const ripple = document.createElement("span");
            const size = Math.max(rect.width, rect.height);
            const x = e.clientX - rect.left - size / 2;
            const y = e.clientY - rect.top - size / 2;

            ripple.style.cssText = `
                    position: absolute;
                    width: ${size}px;
                    height: ${size}px;
                    left: ${x}px;
                    top: ${y}px;
                    background: rgba(255, 255, 255, 0.3);
                    border-radius: 50%;
                    transform: scale(0);
                    animation: ripple 0.6s linear;
                    pointer-events: none;
                `;

            this.style.position = "relative";
            this.style.overflow = "hidden";
            this.appendChild(ripple);

            setTimeout(() => ripple.remove(), 600);
          });
        });

      // Add ripple animation CSS
      const rippleCSS = `
            @keyframes ripple {
                to {
                    transform: scale(4);
                    opacity: 0;
                }
            }
        `;
      const style = document.createElement("style");
      style.textContent = rippleCSS;
      document.head.appendChild(style);

      // Chatbot functionality removed - will be handled by chatbot.js

      // closeChatbot function removed

      // loadChatbotConfig function removed

      // All chatbot configuration functions removed - will be handled by chatbot.js
    </script>

